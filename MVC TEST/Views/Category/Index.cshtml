@model IEnumerable<Category>

<h1>Categories</h1>
<table>
    <thead>
        <tr>
            <th>ID</th>
            <th>Name</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var category in Model)
        {
                <tr>
                    <td>@category.CategoryId</td>
                    <td>@category.CategoryName</td>
                </tr>
        }
    </tbody>
</table>
